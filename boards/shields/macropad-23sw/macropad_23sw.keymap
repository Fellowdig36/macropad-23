#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
    &kp C_PREV &kp C_PLAY_PAUSE &kp C_NEXT
    &kp KP_NUM &kp KP_SLASH &kp KP_ASTERISK &kp KP_MINUS
    &kp N7 &kp N8 &kp N9 &kp PG_UP
    &kp N4 &kp N5 &kp N6 &kp KP_PLUS
    &kp N1 &kp N2 &kp N3 &kp PG_DN
    &kp KP_DOT &kp N0 &kp C_VOLUME_DOWN &kp C_VOLUME_UP
            >;

        };
    };
};